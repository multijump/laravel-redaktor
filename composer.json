{
    "name": "dslabs/laravel-redaktor",
    "description": "Laravel wrapper for Redaktor.",
    "require": {
        "php": "^7.2",
        "dslabs/redaktor": "dev-master@dev",
        "illuminate/http": "^6|^7",
        "illuminate/support": "^6|^7",
        "illuminate/routing": "^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4|^5"
    },
    "autoload": {
        "psr-4": {
            "DSLabs\\LaravelRedaktor\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit tests/"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/projects/redaktor"
        }
    ]
}
